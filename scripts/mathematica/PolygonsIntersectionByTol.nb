Cell["Compute the normal to the polygon", "Section",
 CellChangeTimes->{{3.869467722110895*^9, 
  3.869467733114438*^9}},ExpressionUUID->"23f76540-42c1-4b83-af80-\
a3ea5f1aaca3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ComputeNormal", "[", 
   RowBox[{"pol_", ",", "c_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "np", ",", "i", ",", "i1", ",", "in", ",", "Lvec", ",", "cvec", ",", 
      "xc", ",", "xcenter"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"np", "=", 
      RowBox[{"Length", "[", "pol", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xc", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0", ",", "0"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", "np"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"i1", "=", "i"}], ";", "\[IndentingNewLine]", 
        RowBox[{"in", "=", 
         RowBox[{
          RowBox[{"Mod", "[", 
           RowBox[{"i", ",", "np"}], "]"}], "+", "1"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Lvec", "=", 
         RowBox[{
          RowBox[{"pol", "[", 
           RowBox[{"[", "in", "]"}], "]"}], "-", 
          RowBox[{"pol", "[", 
           RowBox[{"[", "i1", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"xcenter", "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"pol", "[", 
             RowBox[{"[", "in", "]"}], "]"}], "+", 
            RowBox[{"pol", "[", 
             RowBox[{"[", "i1", "]"}], "]"}]}], ")"}], "/", "2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"cvec", "=", 
         RowBox[{"xcenter", "-", "c"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"xc", "+=", 
         RowBox[{"Cross", "[", 
          RowBox[{"cvec", ",", "Lvec"}], "]"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Area \>\"", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Norm", "[", "xc", "]"}], "/", "2"}], "//", "Simplify"}]}],
         "]"}], ";"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"xc", "/", 
      RowBox[{"Norm", "[", "xc", "]"}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.8694678354631157`*^9, 3.869467952732239*^9}, {
