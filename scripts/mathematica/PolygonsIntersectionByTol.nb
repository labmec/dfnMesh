Cell[BoxData[
 RowBox[{
  RowBox[{"PointonIntersection", "[", 
   RowBox[{"polA_", ",", "polB_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "cA", ",", "normalA", ",", "cB", ",", "normalB", ",", "x", ",", "y", ",",
       "z", ",", "xp", ",", "eq1", ",", "eq2", ",", "eq3", ",", "pointproj"}],
      "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cA", "=", 
      RowBox[{
       RowBox[{"ComputeCenter", "[", "polA", "]"}], "//", "N"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cB", "=", 
      RowBox[{
       RowBox[{"ComputeCenter", "[", "polB", "]"}], "//", "N"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"normalA", "=", 
      RowBox[{"ComputeNormal", "[", 
       RowBox[{"polA", ",", "cA"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"normalB", "=", 
      RowBox[{"ComputeNormal", "[", 
       RowBox[{"polB", ",", "cB"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xp", "=", 
      RowBox[{"{", 
       RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"eq1", "=", 
      RowBox[{"normalA", ".", 
       RowBox[{"(", 
        RowBox[{"xp", "-", "cA"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"eq2", "=", 
      RowBox[{"normalB", ".", 
       RowBox[{"(", 
        RowBox[{"xp", "-", "cB"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"eq3", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"xp", "-", "cA"}], ")"}], ".", 
       RowBox[{"Cross", "[", 
        RowBox[{"normalA", ",", "normalB"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<eq1 \>\"", ",", "eq1", ",", "\"\< eq2 \>\"", ",", "eq2", ",", 
         "\"\< eq3 \>\"", ",", "eq3"}], "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"pointproj", "=", 
      RowBox[{
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"eq1", "==", "0"}], ",", 
           RowBox[{"eq2", "==", "0"}], ",", 
           RowBox[{"eq3", "==", "0"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xp", "/.", "pointproj"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
