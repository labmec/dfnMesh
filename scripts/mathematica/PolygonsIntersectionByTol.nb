Cell[BoxData[
 RowBox[{
  RowBox[{"ComputeCenter", "[", "pol_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "np", ",", "c", ",", "totalL", ",", "i", ",", "i1", ",", "in", ",", "L", 
      ",", "xc"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"np", "=", 
      RowBox[{"Length", "[", "pol", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0", ",", "0"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"totalL", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", "np"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"i1", "=", "i"}], ";", "\[IndentingNewLine]", 
        RowBox[{"in", "=", 
         RowBox[{
          RowBox[{"Mod", "[", 
           RowBox[{"i", ",", "np"}], "]"}], "+", "1"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"L", "=", 
         RowBox[{"Norm", "[", 
          RowBox[{
           RowBox[{"pol", "[", 
            RowBox[{"[", "in", "]"}], "]"}], "-", 
           RowBox[{"pol", "[", 
            RowBox[{"[", "i1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xc", "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"pol", "[", 
             RowBox[{"[", "in", "]"}], "]"}], "+", 
            RowBox[{"pol", "[", 
             RowBox[{"[", "i1", "]"}], "]"}]}], ")"}], "/", "2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"c", "+=", 
